<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:osgi="http://www.eclipse.org/gemini/blueprint/schema/blueprint"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd http://www.eclipse.org/gemini/blueprint/schema/blueprint http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd">

    <import resource="classpath*:META-INF/motech/*.xml" />

    <osgi:reference id="stateRecordsDataService"
                    interface="org.motechproject.nms.masterdata.repository.StateRecordsDataService" />

    <osgi:reference id="stateCsvRecordsDataService"
                    interface="org.motechproject.nms.masterdata.repository.StateCsvRecordsDataService" />

    <osgi:reference id="districtCsvRecordsDataService"
                    interface="org.motechproject.nms.masterdata.repository.DistrictCsvRecordsDataService" />

    <osgi:reference id="districtRecordsDataService"
                    interface="org.motechproject.nms.masterdata.repository.DistrictRecordsDataService" />

    <osgi:service ref="locationService"
                  interface="org.motechproject.nms.master.service.LocationService" />

    <osgi:reference id="languageLocationCodeCsvDataService"
                    interface="org.motechproject.nms.masterdata.repository.LanguageLocationCodeCsvDataService" />

    <osgi:service ref="languageLocationCodeService"
                  interface="org.motechproject.nms.masterdata.service.LanguageLocationCodeService" />

    <osgi:reference id="languageLocationCodeDataService"
                    interface="org.motechproject.nms.masterdata.repository.LanguageLocationCodeDataService" />

