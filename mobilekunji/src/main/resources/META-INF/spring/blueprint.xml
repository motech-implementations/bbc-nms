    <osgi:service ref="saveCallDetailsService"
                  interface="org.motechproject.nms.mobilekunji.service.SaveCallDetailsService"/>

    <osgi:service ref="serviceConsumptionFlwService"
                  interface="org.motechproject.nms.mobilekunji.service.ServiceConsumptionFlwService"/>

    <osgi:service ref="serviceConsumptionCallService"
