    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd http://www.eclipse.org/gemini/blueprint/schema/blueprint http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd">

    <import resource="classpath*:META-INF/motech/*.xml" />

    <osgi:service ref="mobileKunjiService"
        interface="org.motechproject.nms.mobilekunji.service.MobileKunjiService" />


    <osgi:reference id="serviceConfiguration"
                    interface="org.motechproject.nms.mobilekunji.repository.ServiceConfigurationRecordDataService" />

    <osgi:reference id="contentUpload"
                    interface="org.motechproject.nms.mobilekunji.repository.ContentUploadRecordDataService" />

    <osgi:reference id="contentUploadCsv"
                    interface="org.motechproject.nms.mobilekunji.repository.ContentUploadCsvRecordDataService" />


    <osgi:reference id="stateCapMapping"
                    interface="org.motechproject.nms.mobilekunji.repository.StateCapMappingRecordDataService" />

    <osgi:reference id="stateCapMappingCsv"
                    interface="org.motechproject.nms.mobilekunji.repository.StateCapMappingCsvRecordDataService" />
</beans>
