<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:osgi="http://www.eclipse.org/gemini/blueprint/schema/blueprint"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd http://www.eclipse.org/gemini/blueprint/schema/blueprint http://www.eclipse.org/gemini/blueprint/schema/blueprint/gemini-blueprint.xsd">

    <import resource="classpath*:META-INF/motech/*.xml" />

    <osgi:service ref="configurationService"
                  interface="org.motechproject.nms.kilkariobd.service.ConfigurationService" />

    <osgi:service ref="outboundCallDetailService"
                  interface="org.motechproject.nms.kilkariobd.service.OutboundCallDetailService" />

    <osgi:service ref="outboundCallFlowService"
                  interface="org.motechproject.nms.kilkariobd.service.OutboundCallFlowService" />

    <osgi:service ref="outboundCallRequestService"
                  interface="org.motechproject.nms.kilkariobd.service.OutboundCallRequestService" />

    <osgi:reference id="contentUploadService"
                    interface="org.motechproject.nms.kilkari.service.ContentUploadService"/>

    <osgi:reference id="subscriptionService"
                    interface="org.motechproject.nms.kilkari.service.SubscriptionService"/>

    <osgi:reference id="languageLocationCodeService"
                    interface="org.motechproject.nms.masterdata.service.LanguageLocationCodeService"/>

    <osgi:reference id="configurationDataService"
                    interface="org.motechproject.nms.kilkariobd.repository.ConfigurationDataService" />

    <osgi:reference id="outboundCallDetailDataService"
                    interface="org.motechproject.nms.kilkariobd.repository.OutboundCallDetailDataService" />

    <osgi:reference id="outboundCallFlowDataService"
                    interface="org.motechproject.nms.kilkariobd.repository.OutboundCallFlowDataService" />

    <osgi:reference id="outboundCallRequestDataService"
                    interface="org.motechproject.nms.kilkariobd.repository.OutboundCallRequestDataService" />

    <osgi:reference id="motechSchedulerService"
                    interface="org.motechproject.scheduler.service.MotechSchedulerService" />


</beans>
